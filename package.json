{
  "name": "resolve-from-fallback",
  "version": "1.0.0",
  "description": "Resolve the path of a module like `require.resolve()` but from some fallback path",
  "author": "余聪 <yucong06@meituan.com>",
  "scripts": {
    "test": "npx ava",
    "test:watch": "npm test -- --watch",
    "preversion": "npm test",
    "build": "npm run clean && run-p --print-label \"build:**\"",
    "dev": "TSC_OPTIONS=\"--watch\" npm run build",
    "build:cjs": "tsc $TSC_OPTIONS --outDir lib",
    "build:tds": "tsc $TSC_OPTIONS --emitDeclarationOnly -d",
    "clean": "rimraf types es lib",
    "prepare": "npm run build",
    "version": "npm run changelog",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS"
    }
  },
  "sideEffects": false,
  "engines": {
    "node": ">=10"
  },
  "files": [
    "es",
    "types",
    "lib",
    "src"
  ],
  "keywords": [
    "余聪",
    "resolve",
    "from",
    "fallback",
    "resolve-from-fallback"
  ],
  "main": "lib",
  "types": "types",
  "license": "MIT",
  "repository": "余聪/resolve-from-fallback",
  "module": "es",
  "ava": {
    "files": [
      "__tests__/**/*.test.ts{,x}"
    ],
    "extensions": [
      "ts",
      "tsx"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@types/node": "^14.14.6",
    "ava": "^3.13.0",
    "conventional-changelog-cli": "^2.1.0",
    "husky": "^4.3.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "pretty-quick": "^3.1.0",
    "rimraf": "^3.0.2",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5"
  },
  "dependencies": {
    "resolve-from": "^5.0.0"
  }
}
